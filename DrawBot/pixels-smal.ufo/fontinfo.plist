	<key>note</key>
	<string>f = CurrentFont()
w = "pixel"
newPage(2100,1000)
translate(0,200)

pixels = []
for g in f:
    if "p." in g.name:
        pixels.append(g)

for l in w:
    for c in f[l].contours:
        p = choice(pixels)
        save()
        translate(c.box[0],c.box[3])
        b=choice(range(0,2,1))
        fill(b,0,0)
        drawGlyph(p)
        restore()
    #drawGlyph(f[l])
    translate(f[l].width,0)</string>
